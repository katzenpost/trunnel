const N_ELEMENTS = 100;
const U8_MAX = 0xff;

struct rgb {
   u8 r;
   u8 g;
   u8 b;
};

const MAX_LEN = 16384;

struct int_constraints {
	u8 version_num IN [ 4, 5, 6 ];
	u16 length IN [ 0..16384 ];
	u16 length2 IN [ 0..MAX_LEN ];
	u8 version_num2 IN [ 1, 2, 4..6, 9..128 ];
};

struct fixed_length_arrays {
    u8 ipv6_addr[16];
    u32 elements[N_ELEMENTS];
    struct rgb colors[2];
    char hostname[0x40];
};
